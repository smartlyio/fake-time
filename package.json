{
  "name": "@smartlyio/fake-time",
  "license": "MIT",
  "version": "1.0.3",
  "description": "fake time and timers",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "yarn jest",
    "render": "yarn ts-node render.ts",
    "build": "rm -rf ./dist/* && yarn lint && yarn test && yarn render && tsc",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "lint:fix": "tslint --fix -c tslint.json --project tsconfig.json",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/fake-time.git"
  },
  "peerDependencies": {
    "jest": "^27.0.0"
  },
  "keywords": [
    "jest",
    "timer",
    "timer",
    "mock time",
    "fake"
  ],
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "16.11.26",
    "jest": "27.5.1",
    "jsverify": "0.8.4",
    "prettier": "2.6.0",
    "ts-jest": "27.1.3",
    "ts-node": "10.7.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "4.6.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "examples/.*\\.ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "preset": "ts-jest",
    "testMatch": null
  }
}
